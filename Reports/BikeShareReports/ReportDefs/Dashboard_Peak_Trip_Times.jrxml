	<queryString>
		<![CDATA[select h.start_hour as hour_of_day, h.trip_count, m.trip_count as max_count
from (
Select start_hour, sum(trip_count) as trip_count
   From trip_volume_today tt
   where tt.system_id = $P{Franchise} $P!{RegionClause}
	  and date(tt.trip_date_today) between $P{Start_Date} and $P{End_Date}
   Group By start_hour
   Order By start_hour ) as h
join
(
Select sum(trip_count) as trip_count
   From trip_volume_today tt
   where tt.system_id = $P{Franchise}  $P!{RegionClause}
	  and date(tt.trip_date_today) between $P{Start_Date} and $P{End_Date}
   Group By start_hour
   Order By sum(trip_count) desc
   limit 1   
) as m 
on 1=1]]>
	</queryString>
